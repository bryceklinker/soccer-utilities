{
  "version": 2,
  "projects": {
    "common-ui": {
      "root": "libs/common-ui",
      "sourceRoot": "libs/common-ui/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/common-ui/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/common-ui"],
          "options": {
            "jestConfig": "libs/common-ui/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "core": {
      "root": "libs/core",
      "sourceRoot": "libs/core/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/core/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/core"],
          "options": {
            "jestConfig": "libs/core/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "data-access": {
      "root": "libs/data-access",
      "sourceRoot": "libs/data-access/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/data-access/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/data-access"],
          "options": {
            "jestConfig": "libs/data-access/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "nest-auth0": {
      "root": "libs/nest-auth0",
      "sourceRoot": "libs/nest-auth0/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/nest-auth0/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/nest-auth0"],
          "options": {
            "jestConfig": "libs/nest-auth0/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "rest-api": {
      "root": "apps/rest-api",
      "sourceRoot": "apps/rest-api/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/node:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/rest-api",
            "main": "apps/rest-api/src/main.ts",
            "tsConfig": "apps/rest-api/tsconfig.app.json",
            "generatePackageJson": true,
            "assets": [
              "apps/rest-api/src/assets",
              "apps/rest-api/src/app/function.json",
              "apps/rest-api/src/host.json"
            ]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false
            }
          }
        },
        "serve": {
          "executor": "@nrwl/node:execute",
          "options": {
            "buildTarget": "rest-api:build"
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/rest-api/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/rest-api"],
          "options": {
            "jestConfig": "apps/rest-api/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "schedules-api": {
      "root": "libs/schedules-api",
      "sourceRoot": "libs/schedules-api/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/schedules-api/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/schedules-api"],
          "options": {
            "jestConfig": "libs/schedules-api/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "schedules-ui": {
      "root": "libs/schedules-ui",
      "sourceRoot": "libs/schedules-ui/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/schedules-ui/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/schedules-ui"],
          "options": {
            "jestConfig": "libs/schedules-ui/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "testing-support": {
      "root": "libs/testing-support",
      "sourceRoot": "libs/testing-support/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/testing-support/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/testing-support"],
          "options": {
            "jestConfig": "libs/testing-support/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "web-ui": {
      "root": "apps/web-ui",
      "sourceRoot": "apps/web-ui/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/web:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/web-ui",
            "index": "apps/web-ui/src/index.html",
            "main": "apps/web-ui/src/main.tsx",
            "polyfills": "apps/web-ui/src/polyfills.ts",
            "tsConfig": "apps/web-ui/tsconfig.app.json",
            "assets": ["apps/web-ui/src/favicon.ico", "apps/web-ui/src/assets"],
            "styles": ["apps/web-ui/src/styles.scss"],
            "scripts": [],
            "webpackConfig": "@nrwl/react/plugins/webpack"
          },
          "configurations": {
            "production": {
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/web:dev-server",
          "options": {
            "buildTarget": "web-ui:build",
            "hmr": true
          },
          "configurations": {
            "production": {
              "buildTarget": "web-ui:build:production",
              "hmr": false
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/web-ui/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/web-ui"],
          "options": {
            "jestConfig": "apps/web-ui/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "web-ui-e2e": {
      "root": "apps/web-ui-e2e",
      "sourceRoot": "apps/web-ui-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/web-ui-e2e/cypress.json",
            "tsConfig": "apps/web-ui-e2e/tsconfig.e2e.json"
          },
          "configurations": {
            "production": {
              "devServerTarget": "web-ui:serve:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/web-ui-e2e/**/*.{js,ts}"]
          }
        }
      }
    }
  },
  "generators": {
    "@nrwl/react": {
      "application": {
        "style": "scss",
        "linter": "eslint",
        "strict": true,
        "babel": true
      },
      "component": {
        "style": "scss"
      },
      "library": {
        "style": "scss",
        "linter": "eslint",
        "strict": true
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/react"
  }
}
