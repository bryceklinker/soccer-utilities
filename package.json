{
  "name": "soccer-utilities",
  "version": "0.1.22",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "nx run-many --target=serve --all --parallel --exclude=workspace",
    "build:tools": "tsc --project ./tools/tsconfig.tools.json",
    "build": "nx run-many --target=build --all --parallel --prod --exclude=workspace",
    "postbuild": "rimraf ./dist/apps/web-ui/assets/settings.json",
    "test:tools": "cross-env TZ=UTC jest --projects ./tools/executors/version-bump",
    "test": "cross-env TZ=UTC nx run-many --target=test --all --no-cache --exclude=workspace",
    "posttest": "npm run test:tools",
    "e2e:wait": "concurrently \"wait-on http://localhost:4200\" \"wait-on http://localhost:3333/.health\"",
    "e2e:local": "dotenv -- concurrently --success first --kill-others \"npm start\" \"npm run e2e:wait && npm run e2e\"",
    "e2e:dev": "dotenv -- concurrently --success first --kill-others \"npm start\" \"npm run e2e:wait && npm run e2e -- --watch\"",
    "e2e": "nx run-many --target=e2e --all --exclude=workspace",
    "update": "nx migrate latest",
    "prebump:version": "npm run build:tools",
    "bump:version": "nx run workspace:version --skip-root-changelog --skip-project-changelog",
    "format": "nx format:write",
    "prepare": "husky install"
  },
